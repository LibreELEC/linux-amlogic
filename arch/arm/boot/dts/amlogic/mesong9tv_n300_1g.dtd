					   8 0x0000003f>;	/*sdio a*/
			amlogic,pins = "GPIOX_0","GPIOX_1","GPIOX_2","GPIOX_3","GPIOX_8","GPIOX_9";
			amlogic,enable-output = <1>; /** 0:output, 1:input */
			amlogic,pullup = <1>;
